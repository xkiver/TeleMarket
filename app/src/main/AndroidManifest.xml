<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.android.gms.samples.vision.barcodereader">

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="TeleMarket"
        android:supportsRtl = "true"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="barcode" />

    <activity android:name=".PrincipalActivity"
        android:label="Telemarket Principal"
        android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>>

    <activity android:name=".SupermercadosActivity"
        android:label="Supermercados TeleMarket"
        android:screenOrientation="portrait">
    </activity>>

    <activity
        android:name=".ConsultarActivity"
        android:label="@string/title_activity_main"
        android:screenOrientation="portrait">
    </activity>

    <activity android:name=".BarcodeCaptureActivity"
        android:label="Captura TeleMarket"
        android:screenOrientation="portrait">
        </activity>>


    <activity android:name=".RespuestaActivity"
        android:label="Respuesta TeleMarket"
        android:screenOrientation="portrait">
        </activity>>

        <activity android:name=".ShopCarActivity"
            android:label="Carro de compra TeleMarket"
            android:screenOrientation="portrait">
        </activity>>

</application>

</manifest>
